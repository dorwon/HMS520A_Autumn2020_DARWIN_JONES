count <- function(vec, x) {
# throw error if vec is not atomic
if(!is.atomic(vec)) {
stop("vec must be an atomic vector")
}
# See the number of times each value in x is in vec and return that vector
counts <- rep(0L, times = length(x))
for(i in x) {
for(j in vec) {
if(vec[j] == x[i]) {
counts[i] = counts
}
}
}
}
