my_var <- function(x, na.rm = TRUE) {
# find the mean
mn <- rep(my_mean(x, na.rm = na.rm), length(x))
#find and square the difference between the mean and each data point
dif <- (mn - x)^2
#sum the result and divide by the length of x - 1
sum(dif) / (length(x) - 1)
}
