binomial_fun <- function(s, n) {
#calulate p^
p_hat <- (1 / n) * s
# calculate sqrt of v_hat, the standard deviation
v_hat <- (p_hat * (1 - p_hat)) / (n-1)
std_dev <- sqrt(v_hat)
return(c(mean = p_hat, standard_dev = std_dev))
}
